{
  "name": "abinbev-e2e-tests",
  "version": "1.0.0",
  "description": "Testes e2e automatizados com Cypress para ABInbev",
  "scripts": {
    "test": "cypress run --headless --browser chrome",
    "test:ui": "cypress open",
    "test:web": "cypress run --spec 'cypress/e2e/web/**/*.cy.js'",
    "test:api": "cypress run --spec 'cypress/e2e/api/**/*.cy.js'",
    "test:chrome": "cypress run --browser chrome",
    "allure:serve": "allure serve allure-results --port 5050 --host 0.0.0.0",
    "allure:report": "allure generate -c allure-results -o allure-report",
    "allure:open": "allure open allure-report"
  },
  "devDependencies": {
    "allure-commandline": "^2.34.1",
    "allure-cypress": "^3.4.1",
    "cypress": "^15.2.0"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "cypress-plugin-api": "^2.11.2"
  }
}
